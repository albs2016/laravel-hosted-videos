{
    "name": "artificertech/laravel-hosted-videos",
    "description": "A package for utilizing third party hosted videos in laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Cole Shirley",
            "email": "cole.shirley@artificertech.com"
        }
    ],
    "homepage": "https://github.com/artificertech/laravel-hosted-videos",
    "keywords": [
        "Laravel",
        "LaravelHostedVideos"
    ],
    "require": {
        "illuminate/support": "~7|~8",
        "artificertech/laravel-renderable": "^1.0-beta"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Artificertech\\LaravelHostedVideos\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artificertech\\LaravelHostedVideos\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Artificertech\\LaravelHostedVideos\\LaravelHostedVideosServiceProvider"
            ],
            "aliases": {
                "LaravelHostedVideos": "Artificertech\\LaravelHostedVideos\\Facades\\LaravelHostedVideos"
            }
        }
    }
}
