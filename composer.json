{
    "name": "artificertech/laravel-embedded-videos",
    "type": "library",
    "description": "Attach third party videos to your models",
    "keywords": [
        "laravel",
        "laravel-embedded-videos",
        "video",
        "youtube",
        "vimeo",
        "embedded"
    ],
    "homepage": "https://github.com/artificertech/laravel-embedded-videos",
    "license": "MIT",
    "authors": [
        {
            "name": "Cole Shirley",
            "email": "cole.shirley@artificertech.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4|>=8.0",
        "laravel/framework": "^5.7|^6|^7|^8"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0",
        "phpunit/phpunit": "^9.3",
        "friendsofphp/php-cs-fixer": "^2.13"
    },
    "autoload": {
        "psr-4": {
            "Artificertech\\LaravelEmbeddedVideos\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artificertech\\LaravelEmbeddedVideos\\tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "fix-style": "vendor/bin/php-cs-fixer fix . --config-file='.php_cs' --verbose"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        },
        "laravel": {
            "providers": [
                "Artificertech\\LaravelEmbeddedVideos\\LaravelEmbeddedVideosServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
